    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "groq-sdk": "^0.34.0",
    "jsdom": "^26.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.87.4",
    "promisify": "^0.0.3",
    "puppeteer": "^24.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "uuid": "^11.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "supabase": "^2.22.4"
  }
}
