{
  "name": "rdv-news-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "fetch": "node fetch-to-airtable.js",
    "fetch:section": "node fetch-to-airtable.js",
    "fetch:all": "node fetch-to-airtable.js --all",
    "fetch:limited": "node fetch-to-airtable.js --all --limit 10",
    "fetch:primera-plana": "node fetch-to-airtable.js primera-plana",
    "fetch:primera-plana:limited": "node fetch-to-airtable.js primera-plana --limit 5",
    "fetch:local": "node fetch-to-airtable.js local",
    "fetch:local:limited": "node fetch-to-airtable.js local --limit 5",
    "fetch:local-facebook": "node fetch-to-airtable.js local-facebook",
    "fetch:local-facebook:limited": "node fetch-to-airtable.js local-facebook --limit 5",
    "fetch:la-sexta": "node fetch-to-airtable.js la-sexta",
    "fetch:la-sexta:limited": "node fetch-to-airtable.js la-sexta --limit 5",
    "fetch:huanguelen": "node fetch-to-airtable.js huanguelen",
    "fetch:huanguelen:limited": "node fetch-to-airtable.js huanguelen --limit 5",
    "fetch:agro": "node fetch-to-airtable.js agro",
    "fetch:agro:limited": "node fetch-to-airtable.js agro --limit 5",
    "fetch:deportes": "node fetch-to-airtable.js deportes",
    "fetch:politica": "node fetch-to-airtable.js politica",
    "fetch:politica:limited": "node fetch-to-airtable.js politica",
    "fetch:economia": "node fetch-to-airtable.js economia",
    "fetch:lifestyle": "node fetch-to-airtable.js lifestyle",
    "fetch:lifestyle:limited": "node fetch-to-airtable.js lifestyle --limit 5",
    "fetch:instituciones": "node fetch-to-airtable.js instituciones",
    "fetch:instituciones:limited": "node fetch-to-airtable.js instituciones --limit 5",
    "fetch:pueblos-alemanes": "node fetch-to-airtable.js pueblos-alemanes",
    "fetch:pueblos-alemanes:limited": "node fetch-to-airtable.js pueblos-alemanes --limit 5",
    "fetch:recetas": "node fetch-to-airtable.js recetas",
    "fetch:recetas:limited": "node fetch-to-airtable.js recetas --limit 5",
    "fetch:cine-series": "node fetch-to-airtable.js cine-series",
    "fetch:cine-series:limited": "node fetch-to-airtable.js cine-series --limit 5",
    "fetch:cultura": "node fetch-to-airtable.js cultura",
    "fetch:cultura:limited": "node fetch-to-airtable.js cultura --limit 5",
    "fetch:turismo": "node fetch-to-airtable.js turismo",
    "fetch:turismo:limited": "node fetch-to-airtable.js turismo --limit 5",
    "fetch:mundo": "node fetch-to-airtable.js mundo",
    "fetch:mundo:limited": "node fetch-to-airtable.js mundo --limit 5",
    "fetch:complete:limited": "node fetch-to-airtable.js --all --limit 10 && node fetch-to-airtable.js instituciones --limit 5",
    "process:social": "node src/scripts/process-social-media.js",
    "fetch:complete": "node fetch-to-airtable.js --all && node fetch-to-airtable.js instituciones",
    "process:all": "node src/scripts/process-social-media.js",
    "system:full": "npm run fetch:complete && npm run process:all",
    "system:limited": "npm run fetch:complete:limited && npm run process:all",
    "social:limited": "npm run fetch:instituciones:limited && npm run process:social",
    "social:full": "npm run fetch:instituciones && npm run process:social",
    "sync:primera-plana": "node src/scripts/sync-primera-plana.js",
    "sync:primera-plana:all": "node src/scripts/sync-primera-plana.js --all",
    "sync:primera-plana:limited": "node src/scripts/sync-primera-plana.js --limit 5",
    "system:full:with-sync": "npm run system:full && npm run sync:primera-plana",
    "test:publish": "node test-publication.js",
    "test:draft": "node test-publication.js --draft",
    "check-airtable": "node check-airtable.js",
    "schedule": "node scheduler.js",
    "vercel-build": "echo 'Build step completed'",
    "supabase:db:diff": "npx supabase db diff --schema public",
    "supabase:db:push": "npx supabase db push",
    "supabase:db:pull": "npx supabase db pull",
    "test:sections": "node src/scripts/test-sections.js"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "API for publishing Airtable records to Supabase",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@google/generative-ai": "^0.24.0",
    "@mozilla/readability": "^0.6.0",
    "@supabase/supabase-js": "^2.49.3",
    "airtable": "^0.12.2",
    "axios": "^1.8.4",
    "canvas": "^3.1.0",
    "cheerio": "^1.0.0",
