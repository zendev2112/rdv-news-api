{
  "version": 2,
  "devCommand": "npm run dev",
  "framework": null,
  "builds": [
    {
      "src": "api/**/*.{js,cjs}",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.cjs"
    }
  ]
}
